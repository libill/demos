<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string name="app_name">demos</string>
    <string name="action_settings">Settings</string>
    <!-- Android 在资源文件（res/strings.xml）定义一维数组，二维数组，多维数组 -->
    
    <!-- 字符串 -->
    <string name="hello_world">Hello world!</string>
    
    <!-- 一维数组 -->
    <string-array name="good">
        　		<item>a</item>
　　        	<item>b</item>
　　        	<item>c</item>
　　        	<item>d</item>
    </string-array>
    
    <!-- 二维数组 -->
 	<array name="two">
        <item>a，b，c，d，e，f，g</item>
        <item>h，i，g，k，l，m，n</item>
    </array>
    
 	<!-- 二维数组 -->
 	<array name="three">
        <item>a，b，c，d，e，f，g</item>
        <item>h，i，g，k，l，m，n</item>
        <item>o，p，q，r，s，t，u</item>
    </array>
    
 	
</resources>
